package test

import (
	"advent_of_code/utils"
	_ "embed"
	"fmt"
	"testing"
)

//go:embed input.txt
var input string

func TestSolution(t *testing.T) {
	l, r := utils.ParseInput(input)

	sl := make([]int, len(l))
	for i, _ := range l {
		for _, p := range r {
			if l[i] == p {
				sl[i]++
			}
		}
	}

	sum := 0
	for _, p := range sl {
		fmt.Println(p)
		sum += p
	}

	if sum != 31 {
		t.Errorf("Expected 31, got %d", sum)
	}
}
